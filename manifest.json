{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "1.0",
    "description": "Right click on Youtube to pull up chords for the song you're listening to",
    "author": "Matt Kyea",
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": ["jquery-3.3.1.js","background.js"],
        "persistent": false
    },
    "context_script":[
        {
            "scripts":["contextScript.js"]
        }
    ],
    "icons": {
        "16": "icon-bitty.png",
        "48": "icon-small.png",
        "128": "icon-large.png"
    }
}